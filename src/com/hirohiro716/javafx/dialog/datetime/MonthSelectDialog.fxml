<AnchorPane fx:id="paneRoot" prefHeight="280.0" prefWidth="300.0" style="-fx-background-color:#fff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <effect>
        <DropShadow height="60.0" radius="30.0" width="60.0" />
    </effect>
    <HBox alignment="CENTER_LEFT" AnchorPane.bottomAnchor="90.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0">
        <ComboBox fx:id="comboBoxYear" prefHeight="30.0" value="2018" />
        <Label text=" / " />
        <ComboBox fx:id="comboBoxMonth" prefHeight="30.0" value="12" />
    </HBox>
    <HBox spacing="10.0" alignment="CENTER_RIGHT" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0">
        <EnterFireButton fx:id="buttonOk" prefHeight="40.0" prefWidth="80.0" text="OK" />
        <EnterFireButton fx:id="buttonCancel" cancelButton="true" prefHeight="40.0" prefWidth="100.0" text="キャンセル" />
    </HBox>
    <Label fx:id="labelTitle" text="タイトル" AnchorPane.topAnchor="30.0" AnchorPane.rightAnchor="0.0" AnchorPane.leftAnchor="30.0" style="-fx-font-size:120%; -fx-font-weight:bold;">
        <padding>
            <Insets top="10.0" right="10.0" bottom="10.0" />
        </padding>
    </Label>
    <AnchorPane fx:id="paneMessage" AnchorPane.bottomAnchor="130.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0" AnchorPane.topAnchor="70.0" />
</AnchorPane>
